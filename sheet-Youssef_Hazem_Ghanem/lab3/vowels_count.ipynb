   "source": [
    "statement = input(\"Enter a statement: \")\n",
    "\n",
    "vowel_count = 0\n",
    "\n",
    "for character in statement:\n",
    "    if character in [\"a\", \"e\", \"i\", \"o\", \"u\"]:\n",
    "        vowel_count += 1\n",
    "\n",
    "if vowel_count > 0:\n",
    "    print(\"The number of vowels in the statement is:\", vowel_count)\n",
    "else:\n",
    "    print(\"No vowels found.\")\n"
   ]
